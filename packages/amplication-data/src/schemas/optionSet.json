{
  "$id": "https://amplication.com/schema/entityfield/properties/optionSet.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "optionSet",
  "type": "object",
  "properties": {
    "options": {
      "type": "array",
      "uniqueItems": true,
      "minItems": 1,
      "items": {
        "type": "object",
        "properties": {
          "label": {
            "type": "string"
          },
          "value": {
            "type": "string",
            "pattern": "^(?![0-9])[a-zA-Z0-9$_]+$"
          }
        }
      }
    }
  }
}
