{
  "cSpell.words": [
    "Amplication",
    "Authorizable",
    "automerge",
    "camelcase",
    "checkmark",
    "cloudbuild",
    "codebrew",
    "curveness",
    "Datagrid",
    "Datasources",
    "deepdash",
    "dockerode",
    "Dropzone",
    "dtos",
    "endregion",
    "flydrive",
    "Formik",
    "IBLOCK",
    "linkcode",
    "loglevel",
    "nestjs",
    "oclif",
    "Octokit",
    "ofek",
    "openapi",
    "postpack",
    "posttest",
    "prebuild",
    "Queryable",
    "Repos",
    "rmwc",
    "scrollbar",
    "scrollbars",
    "Signup",
    "slynova",
    "teleporter",
    "uncontained",
    "unelevated",
    "unmark",
    "UNREQUIRED",
    "upsert",
    "Xarrow",
    "xarrows",
    "xsmall"
  ],
  "eslint.workingDirectories": [
    {
      "pattern": "./packages/*"
    }
  ],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true
  },
  "terraform.languageServer": {},
  "editor.formatOnSave": true,
  "cSpell.ignoreWords": ["exshql"],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[prisma]": {
    "editor.defaultFormatter": null
  },
  "[terraform]": {
    "editor.defaultFormatter": null
  }
}
