{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/amplication-scheduler/src",
  "projectType": "application",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/amplication-scheduler/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/amplication-scheduler"],
      "options": {
        "jestConfig": "packages/amplication-scheduler/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "prebuild": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "ts-node scripts/generate-help",
          "json2ts -i src/config.schema.json -o src/config.ts"
        ],
        "cwd": "packages/amplication-scheduler",
        "parallel": false
      }
    },
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["prebuild"],
      "options": {
        "outputPath": "dist/packages/amplication-scheduler",
        "main": "packages/amplication-scheduler/src/main.ts",
        "tsConfig": "packages/amplication-scheduler/tsconfig.app.json"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "amplication-scheduler:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "amplication-scheduler:build:production"
        }
      }
    }
  },
  "tags": []
}
